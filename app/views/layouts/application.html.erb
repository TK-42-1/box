<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title))%></title>
  <%= render 'layouts/favicon' %>
  <link rel="stylesheet" href="fonts/Code39Azalea.css">
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>
<body>
	<%= render 'layouts/header' unless @print %>
	<div class="container">
		<% flash.each do |key, value| %>
			<div class="alert alert-<%= key %> "><%= value %> <button type="button" class="close" data-dismiss="alert">&times;</button></div>
		<% end %>
		<%= yield %>
		<%= render 'layouts/footer' unless @print %>
		<%= debug(params) if Rails.env.development? unless @print %>	
	</div>
</body>
</html>
